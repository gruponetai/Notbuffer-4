<advancedsettings>
<gui>
	<algorithmdirtyregions>3</algorithmdirtyregions>
	<nofliptimeout>0</nofliptimeout>
</gui>
<network>
	<autodetectpingtime>30</autodetectpingtime>
	<alwaysforcebuffer>true</alwaysforcebuffer>
	<curlclienttimeout>10</curlclienttimeout>
	<curllowspeedtime>20</curllowspeedtime>
	<disableipv6>true</disableipv6>
	<curlretries>2</curlretries>
</network>
<cache>
	<alwaysforcebuffer>true</alwaysforcebuffer>
	<buffermode>0</buffermode>
	<freememorycachepercent>5</freememorycachepercent>
	<memorysize>167772160</memorysize>
	<readfactor>10</readfactor>
</cache>
<pvr>
	<timecorrection>0</timecorrection>
	<autoscaniconsuserset>false</autoscaniconsuserset>
	<channeliconsautoscan>true</channeliconsautoscan>
	<numericchannelswitchtimeout>1000</numericchannelswitchtimeout>
</pvr>
<epg>
	<activetagcheckinterval>60</activetagcheckinterval>
	<displayupdatepopup>true</displayupdatepopup>
	<lingertime>1440</lingertime>
	<retryinterruptedupdateinterval>30</retryinterruptedupdateinterval>
	<updatecheckinterval>300</updatecheckinterval>
</epg>
<Videoscanner>
	<ignoreerrors>true</ignoreerrors>
</Videoscanner>
<checkdxvacompatibility>false</checkdxvacompatibility>
<samba>
	<clienttimeout>10</clienttimeout>
	<statfiles>false</statfiles>
</samba>
<measurerefreshrate>true</measurerefreshrate>
<forcedswaptime>1</forcedswaptime>
<video>
	<adjustrefreshrate>
		<override>
			<fps>23.976</fps>
			<refresh>60.0</refresh>
			<fps>23</fps>
			<refresh>50</refresh>
			<fpsmin>29</fpsmin>
			<fpsmax>30</fpsmax>
			<refreshmin>59</refreshmin>
			<refreshmax>61</refreshmax>
			<fpsmin>29</fpsmin>
			<fpsmax>30.2</fpsmax>
			<refreshmin>59</refreshmin>
			<refreshmax>60</refreshmax>
		</override>
		<fallback>
			<refresh>50</refresh>
		</fallback>
	</adjustrefreshrate>
	<latency>
		<delay>23</delay>
		<refresh>
			<min>23</min>
			<max>24</max>
			<delay>125</delay>
			<rate>24</rate>
			<delay>125</delay>
		</refresh>
	</latency>
	<stagefright>
		<useavccodec>-1</useavccodec>
		<usemp4codec>-1</usemp4codec>
		<usempeg2codec>-1</usempeg2codec>
		<useswrenderer>false</useswrenderer>
		<usevc1codec>-1</usevc1codec>
		<usevpxcodec>-1</usevpxcodec>
	</stagefright>
</video>
<skiploopfilter>0</skiploopfilter>
<video>
	<allowmpeg4vaapi>true</allowmpeg4vaapi>
	<dxvaallowhqscaling>false</dxvaallowhqscaling>
	<autoscalemaxfps>30</autoscalemaxfps>
	<checkdxvacompatibility>true</checkdxvacompatibility>
	<fpsdetect>1</fpsdetect>
	<percentseekbackward>-2</percentseekbackward>
	<percentseekbackwardbig>-4</percentseekbackwardbig>
	<percentseekforward>2</percentseekforward>
	<percentseekforwardbig>4</percentseekforwardbig>
	<timeseekbackward>-600</timeseekbackward>
	<timeseekbackwardbig>-1200</timeseekbackwardbig>
	<timeseekforward>600</timeseekforward>
	<timeseekforwardbig>1200</timeseekforwardbig>
</video>
</advancedsettings>
